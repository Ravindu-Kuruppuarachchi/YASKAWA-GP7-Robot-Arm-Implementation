<?xml version="1.0" ?>
<robot name="yaskawa_gp7">
<link name="world"/>

  <joint name="world_fixed" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>
  <link name="base_link">
    <visual>
      <geometry>
        <mesh filename="package://gp7_sim/meshes/gp7/visual/gp7_base_axis.stl"/>
      </geometry>
      <material name="blue">
        <color rgba="0 0 1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://gp7_sim/meshes/gp7/collision/gp7_base_axis.stl"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="10.0"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
  </link>

  <link name="vendor_base_link"/>

  <link name="link_s">
    <visual>
      <geometry>
        <mesh filename="package://gp7_sim/meshes/gp7/visual/gp7_s_axis.stl"/>
      </geometry>
      <material name="silver">
        <color rgba="0.8 0.8 0.8 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://gp7_sim/meshes/gp7/collision/gp7_s_axis.stl"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="5.0"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.05" ixy="0" ixz="0" iyy="0.05" iyz="0" izz="0.05"/>
    </inertial>
  </link>

  <link name="link_l">
    <visual>
      <geometry>
        <mesh filename="package://gp7_sim/meshes/gp7/visual/gp7_l_axis.stl"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://gp7_sim/meshes/gp7/collision/gp7_l_axis.stl"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="4.0"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.04" ixy="0" ixz="0" iyy="0.04" iyz="0" izz="0.04"/>
    </inertial>
  </link>

  <link name="link_u">
    <visual>
      <geometry>
        <mesh filename="package://gp7_sim/meshes/gp7/visual/gp7_u_axis.stl"/>
      </geometry>
      <material name="silver"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://gp7_sim/meshes/gp7/collision/gp7_u_axis.stl"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="4.0"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.04" ixy="0" ixz="0" iyy="0.04" iyz="0" izz="0.04"/>
    </inertial>
  </link>

  <link name="link_r">
    <visual>
      <geometry>
        <mesh filename="package://gp7_sim/meshes/gp7/visual/gp7_r_axis.stl"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://gp7_sim/meshes/gp7/collision/gp7_r_axis.stl"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2.0"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.02"/>
    </inertial>
  </link>

  <link name="link_b">
    <visual>
      <geometry>
        <mesh filename="package://gp7_sim/meshes/gp7/visual/gp7_b_axis.stl"/>
      </geometry>
      <material name="silver"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://gp7_sim/meshes/gp7/collision/gp7_b_axis.stl"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <link name="link_t">
    <visual>
      <geometry>
        <mesh filename="package://gp7_sim/meshes/gp7/visual/gp7_t_axis.stl"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://gp7_sim/meshes/gp7/collision/gp7_t_axis.stl"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.005" ixy="0" ixz="0" iyy="0.005" iyz="0" izz="0.005"/>
    </inertial>
  </link>

  <link name="flange">
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

  <joint name="joint_s" type="revolute">
    <parent link="base_link"/>
    <child link="link_s"/>
    <origin xyz="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000" lower="-2.967" upper="2.967" velocity="6.54"/>
  </joint>

  <joint name="joint_vendor_base" type="fixed">
    <parent link="base_link"/>
    <child link="vendor_base_link"/>
    <origin xyz="0 0 0.330"/>
  </joint>

  <joint name="joint_l" type="revolute">
    <parent link="link_s"/>
    <child link="link_l"/>
    <origin xyz="0.040 0 0.330"/>
    <axis xyz="0 1 0"/>
    <limit effort="1000" lower="-1.134" upper="2.530" velocity="5.49"/>
  </joint>

  <joint name="joint_u" type="revolute">
    <parent link="link_l"/>
    <child link="link_u"/>
    <origin xyz="0 0 0.445"/>
    <axis xyz="0 1 0"/>
    <limit effort="1000" lower="-2.024" upper="4.450" velocity="7.15"/>
  </joint>

  <joint name="joint_r" type="revolute">
    <parent link="link_u"/>
    <child link="link_r"/>
    <origin xyz="0.0 0 0.040"/>
    <axis xyz="1 0 0"/>
    <limit effort="1000" lower="-3.316" upper="3.316" velocity="9.59"/>
  </joint>

  <joint name="joint_b" type="revolute">
    <parent link="link_r"/>
    <child link="link_b"/>
    <origin xyz="0.440 0 0"/>
    <axis xyz="0 1 0"/>
    <limit effort="1000" lower="-2.356" upper="2.356" velocity="9.59"/>
  </joint>

  <joint name="joint_t" type="revolute">
    <parent link="link_b"/>
    <child link="link_t"/>
    <origin xyz="0.080 0 0"/>
    <axis xyz="1 0 0"/>
    <limit effort="1000" lower="-6.283" upper="6.283" velocity="17.45"/>
  </joint>

  <joint name="joint_flange" type="fixed">
    <parent link="link_t"/>
    <child link="flange"/>
    <origin rpy="0 -1.570796326795 3.14159265359"/>
  </joint>

  <link name="gripper_link">
    <visual>
      <geometry>
        <box size="0.05 0.05 0.1"/>
      </geometry>
      <material name="grey">
        <color rgba="0.5 0.5 0.5 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.05 0.05 0.1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>

  <joint name="tool0_to_gripper" type="fixed">
    <parent link="flange"/> <child link="gripper_link"/>
    <origin xyz="0 0 0.05" rpy="0 0 0"/>
  </joint>
<gazebo>
    <plugin filename="gz-sim-joint-position-controller-system" name="gz::sim::systems::JointPositionController">
      <joint_name>joint_s</joint_name>
      <topic>/model/gp7_robot/joint/joint_s/cmd_pos</topic>
      <p_gain>1000</p_gain><i_gain>100</i_gain><d_gain>50</d_gain>
    </plugin>
    <plugin filename="gz-sim-joint-position-controller-system" name="gz::sim::systems::JointPositionController">
      <joint_name>joint_l</joint_name>
      <topic>/model/gp7_robot/joint/joint_l/cmd_pos</topic>
      <p_gain>5000</p_gain><i_gain>100</i_gain><d_gain>100</d_gain>
    </plugin>
    <plugin filename="gz-sim-joint-position-controller-system" name="gz::sim::systems::JointPositionController">
      <joint_name>joint_u</joint_name>
      <topic>/model/gp7_robot/joint/joint_u/cmd_pos</topic>
      <p_gain>3000</p_gain><i_gain>50</i_gain><d_gain>50</d_gain>
    </plugin>
    <plugin filename="gz-sim-joint-position-controller-system" name="gz::sim::systems::JointPositionController">
      <joint_name>joint_r</joint_name>
      <topic>/model/gp7_robot/joint/joint_r/cmd_pos</topic>
      <p_gain>100</p_gain><i_gain>10</i_gain><d_gain>1</d_gain>
    </plugin>
    <plugin filename="gz-sim-joint-position-controller-system" name="gz::sim::systems::JointPositionController">
      <joint_name>joint_b</joint_name>
      <topic>/model/gp7_robot/joint/joint_b/cmd_pos</topic>
      <p_gain>100</p_gain><i_gain>10</i_gain><d_gain>1</d_gain>
    </plugin>
    <plugin filename="gz-sim-joint-position-controller-system" name="gz::sim::systems::JointPositionController">
      <joint_name>joint_t</joint_name>
      <topic>/model/gp7_robot/joint/joint_t/cmd_pos</topic>
      <p_gain>100</p_gain><i_gain>10</i_gain><d_gain>1</d_gain>
    </plugin>
  </gazebo>
</robot>
